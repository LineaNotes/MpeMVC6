@using System.Threading.Tasks
@model IEnumerable<WebApplication3.Models.ApplicationUser>

@{
	ViewData["Title"] = "Admin";
}

<h3>W.I.P.</h3>
<article class="row body-content">
	<div class="col-md-offset-3 col-md-6">
		<table class="table">
			<tr>
				<th>
					@Html.DisplayNameFor(model => model.Email)
				</th>
				<th></th>
			</tr>

			@foreach (var item in Model)
			{
				<tr>
					<td>
						@Html.DisplayFor(modelItem => item.Email)
					</td>
					<td>
						<a asp-action="Edit" asp-route-id="@item.Id">Uredi</a> |
						<a asp-action="Details" asp-route-id="@item.Id">Podrobno</a> |
						<a asp-action="Delete" asp-route-id="@item.Id">Bri&#353;i</a>
					</td>
				</tr>
			}
		</table>
	</div>
</article>